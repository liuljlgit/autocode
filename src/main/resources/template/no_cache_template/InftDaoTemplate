package ${daoPackageOutPath};

import java.util.*;
import org.apache.ibatis.annotations.Param;
import org.springframework.stereotype.Repository;
import com.cloud.common.complexquery.QueryExample;
import ${entityFullPath};

/**
  * 接口类 I${entityName}Dao
  * @author lijun
  */
@Repository
public interface I${entityName}Dao {

    /**
     * 根据主键获取对象
     * @param ${entityId}
     * @return
     */
    ${entityName} load${entityName}ByKey(${entityIdType} ${entityId});

    /**
     * 新增对象
     * @param ${entityObj}
     * @return
     */
    Integer add${entityName}(${entityName} ${entityObj});

    /**
     * 批量新增对象
     * @param list
     */
    void add${entityName}List(List<${entityName}> list);

    /**
     * 更新对象
     * @param ${entityObj}
     * @return
     */
    Integer update${entityName}(${entityName} ${entityObj});

    /**
     * 更新对象:全更新
     * @param ${entityObj}
     * @return
     */
    Integer fullUpdate${entityName}(${entityName} ${entityObj});

    /**
     * 批量更新
     * @param list
     */
    void update${entityName}List(List<${entityName}> list);

    /**
     * 批量更新:全更新
     * @param list
     */
    void fullUpdate${entityName}List(List<${entityName}> list);

    /**
     * 删除对象
     * @param ${entityId}
     * @return
     */
    Integer delete${entityName}ById(${entityIdType} ${entityId});

    /**
     * 批量删除对象
     * @param ids
     */
    void delete${entityName}ByIds(List<${entityIdType}> ids);

    /**
     * 根据普通查询批量删除
     * @param ${entityObj}
     */
    void delete${entityName}(${entityName} ${entityObj});

    /**
     * 根据复杂查询批量删除
     * @param queryExample
     */
    void delete${entityName}Example(QueryExample queryExample);

    /**
     * 查询记录总数
     * @param ${entityObj}
     * @return
     */
    Long get${entityName}Count(${entityName} ${entityObj});

    /**
     * 分页查询列表
     * @param ${entityObj}
     * @return
     */
    List<${entityName}> get${entityName}List(${entityName} ${entityObj});

    /**
     * 查询记录总数
     * @param queryExample
     * @return
     */
     Long get${entityName}CountExample(QueryExample queryExample);

    /**
     * 分页查询列表
     * @param queryExample
     * @return
     */
     List<${entityName}> get${entityName}ListExample(QueryExample queryExample);

     /**
      * 根据ID列表从数据库中查询列表
      * @param list
      * @return
      */
      List<${entityName}> find${entityName}ListByIds(List<${entityIdType}> list);
}
